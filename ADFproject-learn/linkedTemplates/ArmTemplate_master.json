{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFproject-learn"
		},
		"ls_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa'"
		},
		"ls_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdl'"
		},
		"ls_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa2coviddl.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/testing.csv"
		},
		"tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "testing.csv\n"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/167ee12a-a4da-41bd-b7d3-c51cef19c0cd/resourceGroups/rg-ad-covidproject/providers/Microsoft.Storage/storageAccounts/adfcovid"
		},
		"trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/country_response.csv"
		},
		"trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "country_response.csv"
		},
		"trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/hospital_admissions.csv"
		},
		"trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFproject-learn_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa_connectionString')]"
					},
					"ls_adls_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdl_accountKey')]"
					},
					"ls_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tigger_cases_deaths_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					},
					"trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('trigger_country_response_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('trigger_hospital_admissions_data_properties_pipe_ingest_ecdc_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}